<?xml version="1.0" encoding="UTF-8"?>
<sect1 version="5.0" xmlns="http://docbook.org/ns/docbook" xmlns:ns6="http://www.w3.org/1999/xlink" xmlns:ns5="http://www.w3.org/1999/xhtml" xmlns:ns4="http://www.w3.org/2000/svg"    xmlns:m="http://www.w3.org/1998/Math/MathML" xmlns:ns="http://docbook.org/ns/docbook">
    <title>%component_title%</title>
    <subtitle>%subtitle%</subtitle>

    <sect2>
        <title>%metadataString%</title>
        <informaltable>
            <tgroup cols="3">
                <colspec colname="c1" colnum="1" colwidth="1.0*"/>
                <colspec colname="c2" colnum="2" colwidth="2.39*"/>
                <colspec colname="newCol3" colnum="3" colwidth="2.39*"/>
<!--            <thead>
                <row>
                    <entry namest="c1" nameend="c2">%metadataString%</entry>
                </row>
            </thead>-->
                <tbody>
                    <row>
                        <entry>%classificationString%</entry>
                        <entry namest="c2" nameend="newCol3">%classification%</entry>
                    </row>
                    <row>
                        <entry>%packageString%</entry>
                        <entry namest="c2" nameend="newCol3">%package%</entry>
                    </row>
                    <row>
                        <entry>%authorString%</entry>
                        <entry namest="c2" nameend="newCol3">%author%</entry>
                    </row>
                    <row>
                        <entry>%versionString%</entry>
                        <entry namest="c2" nameend="newCol3">%version%</entry>
                    </row>
                    <row>
                        <entry>%dateString%</entry>
                        <entry namest="c2" nameend="newCol3">%date%</entry>
                    </row>
                </tbody>
            </tgroup>
        </informaltable>
    </sect2>  
    <sect2>
        <title>%variableTitle%</title>
        %inputvariableBlock%
        %statevariableBlock%
        %outputvariableBlock%
