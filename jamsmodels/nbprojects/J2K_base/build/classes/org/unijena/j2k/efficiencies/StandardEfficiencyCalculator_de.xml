                  <m:mn>2</m:mn>
                </m:msup>
              </m:mrow>

              <m:mrow>
                <m:munderover>
                  <m:mo>∑</m:mo>

                  <m:mrow>
                    <m:mi>i</m:mi>

                    <m:mo>=</m:mo>

                    <m:mn>1</m:mn>
                  </m:mrow>

                  <m:mi>n</m:mi>
                </m:munderover>

                <m:msup>
                  <m:mrow>
                    <m:mo>(</m:mo>

                    <m:mrow>
                      <m:mrow>
                        <m:mi>ln</m:mi>

                        <m:mo>⁡</m:mo>

                        <m:msub>
                          <m:mi>o</m:mi>

